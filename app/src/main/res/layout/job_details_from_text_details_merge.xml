<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_agent"
                android:tag="Name"/>

            <EditText
                android:id="@+id/etFromFullName"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_mail"
                android:tag="Email"/>

            <EditText
                android:id="@+id/etFromEmail"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_phone"
                android:tag="Phone"/>

            <EditText
                android:id="@+id/etFromPhone"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_rss_box"
                android:tag="Cellphone"/>

            <EditText
                android:id="@+id/etFromCellPhone"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_fax"
                android:tag="Fax"/>

            <EditText
                android:id="@+id/etFromFax"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">


            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_home"
                android:tag="Address"/>

            <EditText
                android:id="@+id/etFromAddress"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="35dp"
                android:layout_weight="4"
                android:background="@drawable/form_borders_gray"
                android:gravity="center_vertical"/>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_index"
                android:tag="Apt"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:gravity="center_vertical">

                <EditText
                    android:id="@+id/etFromApt"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="35dp"
                    android:layout_weight="1"
                    android:background="@drawable/form_borders_gray"
                    android:gravity="center_vertical"/>

                <com.miami.moveforless.customviews.HelpImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/swap_vertical"
                    android:tag="Elevator Reservation"/>

                <CheckBox
                    android:id="@+id/cbHaveFromElevator"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/tvElevatorFromStart"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:text="7 AM"
                    android:textColor="@color/cyan_dark"/>

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_dn"/>

                <TextView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:text=" - "
                    android:textColor="@color/cyan_dark"/>

                <TextView
                    android:id="@+id/tvElevatorFromEnd"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:text="6 AM"
                    android:textColor="@color/cyan_dark"/>

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_dn"/>
            </LinearLayout>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">


            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_domain"
                android:tag="City"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:gravity="center_vertical">

                <EditText
                    android:id="@+id/etFromCity"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="35dp"
                    android:layout_weight="1"
                    android:background="@drawable/form_borders_gray"
                    android:gravity="center_vertical"/>


                <com.miami.moveforless.customviews.HelpImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_umbrella"
                    android:tag="Move Out Insurance"/>

                <CheckBox
                    android:id="@+id/cbIsFromInsurance"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>

            </LinearLayout>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="15dp"
            android:paddingLeft="50dp">

            <com.miami.moveforless.customviews.HelpImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/icn_state"
                android:tag="State"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_gravity="center_vertical"
                android:layout_weight="5"
                android:gravity="center_vertical">

                <EditText
                    android:id="@+id/etFromState_From"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="35dp"
                    android:layout_weight="1"
                    android:background="@drawable/form_borders_gray"
                    android:gravity="center_vertical"/>

                <EditText
                    android:id="@+id/etZipCodeFrom2"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/form_borders_gray"
                    android:gravity="center_vertical"/>

                <com.miami.moveforless.customviews.HelpImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_storage"
                    android:tag="Storage"/>

                <CheckBox
                    android:id="@+id/cbHaveInventoryFrom"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>

                <com.miami.moveforless.customviews.HelpImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_man"
                    android:tag="Referral"/>

                <CheckBox
                    android:id="@+id/cbIsReferralFrom"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>

                <com.miami.moveforless.customviews.HelpImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/icn_refreshes"
                    android:tag="Repeated"/>

                <CheckBox
                    android:id="@+id/cbIsFromRepeated"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>

            </LinearLayout>

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</merge>